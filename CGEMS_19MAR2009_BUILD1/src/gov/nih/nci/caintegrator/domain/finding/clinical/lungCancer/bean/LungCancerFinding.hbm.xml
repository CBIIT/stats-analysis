<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
		<class
		name="gov.nih.nci.caintegrator.domain.finding.clinical.lungCancer.bean.LungCancerFinding"
		lazy="true" table="LUNG_CANCER_FINDING">
		
		<id name="id"  type="java.lang.String"  column="FINDING_ID">			   
		</id>	
		
		<property name="Frozen_Acinar" type="java.lang.Boolean" column="Frozen_Acinar"/>
    	<property name="Frozen_Papillary" type="java.lang.Boolean" column="Frozen_Papillary"/>
    	<property name="Frozen_Concordant" type="java.lang.Boolean" column="Frozen_Concordant"/>
    	<property name="Frozen_BAC" type="java.lang.String" column="Frozen_BAC"/>
    	<property name="Frozen_Micropapillary" type="java.lang.Boolean" column="Frozen_Micropapillary"/>
    	<property name="Percent_Tumor" type="java.lang.Integer" column="Percent_Tumor"/>
    	<property name="Frozen_Solid" type="java.lang.Boolean" column="Frozen_Solid"/>
    	<property name="ClearCell" type="java.lang.Float" column="Clear_Cell"/>
    	<property name="Fetal" type="java.lang.Float" column="Fetal"/>
        <property name="Signet_Ring" type="java.lang.Float" column="Signet_Ring"/>
        <property name="Papillary" type="java.lang.Integer" column="Papillary"/>
