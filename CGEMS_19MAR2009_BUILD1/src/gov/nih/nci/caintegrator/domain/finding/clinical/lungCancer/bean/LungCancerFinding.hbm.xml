        <property name="Colloid" type="java.lang.Integer" column="Colloid"/>
        <property name="Solid" type="java.lang.Integer" column="Solid"/>
       	<property name="BAC" type="java.lang.Integer" column="BAC"/>       	
 	    <property name="BAC_Type" type="java.lang.String" column="BAC_Type"/>
	    <property name="Pahology_DX" type="java.lang.String" column="Pahology_DX"/>
	    <property name="Path_Review" type="java.lang.String" column="Path_Review"/>
	    <property name="TSP_Normal_DNA_ID" type="java.lang.Integer" column="TSP_Normal_DNA_ID"/>
	    <property name="Normal_DNA_Source" type="java.lang.String" column="Normal_DNA_Source"/>
	
	</class>
</hibernate-mapping>