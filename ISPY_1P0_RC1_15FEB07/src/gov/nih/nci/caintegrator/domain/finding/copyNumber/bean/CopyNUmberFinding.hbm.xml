	<union-subclass
		name="gov.nih.nci.caintegrator.domain.finding.variation.somaticMutation.bean.CopyNUmberFinding"
		extends="gov.nih.nci.caintegrator.domain.finding.bean.VariationFinding"
		lazy="true" table="CNA_FINDING">
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
			
<hibernate-mapping package="gov.nih.nci.caintegrator.domain.finding.variation.somaticMutation.bean">
   <class name="CopyNUmberFinding" 
    	table="CNA_FINDING"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.String" 
           column="PARTICIPANT_DID">
           <generator class="assigned" />
       </id>
		
		<property name="abnomalityStatus" type="java.lang.String" column="Abnomality_Status"/>
    	<property name="arrayType" type="java.lang.String" column="Array_Type"/>
    	<property name="call" type="java.lang.String" column="call"/>
    	<property name="copyNumber" type="java.lang.Double" column="Copy_Number"/>
    	<property name="copyNumberPvalue" type="java.lang.Double" column="Copy_Number_Pvalue"/>
    	<one-to-one name="CNAAnnotation" class="gov.nih.nci.caintegrator.domain.finding.copyNumber.bean.CopyNumberAnnotation" outer-join="auto" /> 
               	
	</class>
</hibernate-mapping>