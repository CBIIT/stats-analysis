<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="gov.nih.nci.caintegrator.domain.epidemiology.bean.EnvironmentalFactor" 
    	table="ENV_FACTOR"
        lazy="true">

	    <id name="id" 
	    	column="ID">
	        <generator class="native"/>
	    </id>
	    
        <property name="exposureLevel" 
        	column="EXPOSURE_LEVEL"/>
        	
        <property name="exposureArea" 
        	column="EXPOSURE_AREA"/>
        	
        <property name="exposureAgeGroup" 
        	column="EXPOSURE_AGE_GROUP"/>
        	
        <property name="exposureType" 
        	column="EXPOSURE_TYPE"/>
        	


	</class>
</hibernate-mapping>