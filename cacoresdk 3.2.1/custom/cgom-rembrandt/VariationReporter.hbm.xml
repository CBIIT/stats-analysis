<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping
	package="gov.nih.nci.caintegrator.domain.annotation.snp">
	<class name="VariationReporter" table="SNP_DIM"
		lazy="true">

		<!-- id -->
		<id name="id" type="java.lang.Long" column="SNPANNO_ID">
			<generator class="assigned" />
		</id>

		<property name="name" type="java.lang.String" column="REPORTER_NAME" />
		<property name="platform" type="java.lang.String" column="REPORTER_PLATFORM" />
		<property name="type" type="java.lang.String" column="REPORTER_TYPE" />
		<component name="snpAnnotation"
			class="gov.nih.nci.caintegrator.domain.annotation.snp.SNPAnnotation">
			<property name="chromosomeLocation"
			    type="java.lang.Integer"
				column="PHYSICAL_LOCATION" />
			<property name="chromosomeName" 
				type="java.lang.String" 
				column="CHROMOSOME" />
			<property name="dbsnpBuild"  
				type="java.lang.String" 
				column="DBSNP_BUILD" />
			<property name="dbsnpId"  
				type="java.lang.String"
				column="DBSNPID" />
			<property name="genomeBuild"  
			type="java.lang.String" 
			column="GENOME_BUILD" />
			<!--property name="referenceSequence"
				column="SNP_REF_SEQUENCE" />
			<property name="referenceStrand" column="SNP_STRAND" />
			<property name="secondaryIdentifier"
				column="SNP_SECONDARY_IDENTIFIER" /-->
			<set name="geneBiomarkerCollection" table="GENE_SNP_ASSO"
				batch-size="60">
				<cache usage="read-write" />
				<key column="SNPANNO_ID" />
				<many-to-many
					class="gov.nih.nci.caintegrator.domain.annotation.gene.GeneBiomarker"
					outer-join="true" column="GENE_DIM_ID" />
			</set>
		</component>


	</class>
</hibernate-mapping>