<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.analysis.snp">
   <class name="OddsRatio"
    	table="ODDS_RATIO"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.Long"
           column="ODDS_RATIO_ID">
           <generator class="assigned" />
       </id>

        <!-- homozygoteOddsRatio-->
         <property name="homozygoteOddsRatio"
                 type="java.lang.Double"
                 column="HOMOZYGOTE"
                 insert="false"
                 update="false" />

        <!-- name -->
       <property name="name" 
                 type="java.lang.String" 
                 column="ODDS_RATIO_NAME" 
                 insert="false" 
                 update="false" />
                 
       <!-- heterozygoteOddsRatio -->                 
       <property name="heterozygoteOddsRatio" 
                 type="java.lang.Double" 
                 column="HETEROZYGOTE" 
                 insert="false" 
                 update="false" />     
                 
