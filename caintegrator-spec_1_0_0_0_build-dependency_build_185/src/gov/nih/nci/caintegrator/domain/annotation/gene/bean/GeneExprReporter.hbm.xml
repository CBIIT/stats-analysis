<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping
	package="gov.nih.nci.caintegrator.domain.annotation.gene.bean">
	<class name="GeneExprReporter" table="GENE_EXPR_REPORTER" lazy="true">

		<!-- id -->
		<id name="id" column="REPORTER_ID">
			<generator class="assigned" />
		</id>

		<property name="name" column="REPORTER_NAME" />
		<property name="type" column="REPORTER_TYPE" />
		<property name="platform" column="REPORTER_PLATFORM" />

		<many-to-one name="geneBioMarker" class="gov.nih.nci.caintegrator.domain.annotation.gene.bean.GeneBiomarker" column="GENE_ID"/>
	</class>
</hibernate-mapping>