    Description: Hibernate mapping file for IHCFinding
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="gov.nih.nci.caintegrator.domain.finding.protein.ihc.bean">	

	<union-subclass
		name="IHCFinding"
		extends="gov.nih.nci.caintegrator.domain.finding.bean.SpecimenBasedMolecularFinding"
		abstract="true">	
		
	
	<!--  1-1 relationship -->
    <many-to-one name="proteinBiomarker"
            class="gov.nih.nci.caintegrator.domain.annotation.protein.bean.ProteinBiomarker"
            column="PROTEIN_BIOMARKER_ID"/> 
          

		
	</union-subclass>
</hibernate-mapping>
