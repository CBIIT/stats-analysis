\pard\intbl\nowidctlpar\hyphpar0\sb60\sa60\sl240\slmult0\cf2\f3\fs18 Configure log4j\par
\pard\intbl\nowidctlpar\hyphpar0\sb60\sa60\sl-200\slmult0\cf0\f0\fs20 <appender name="WEBGENOME_FILE" class="org.apache.log4j.DailyRollingFileAppender">\par
         <param name="File" value="$\{jboss.server.home.dir\}/log/bioAssayService.log"/>\par
         <param name="Append" value="true"/>\par
         <layout class="org.apache.log4j.PatternLayout">\par
            <param name="ConversionPattern" value="%d %-5p [%c] %m%n"/>\par
         </layout>\par
    </appender>\par
\par
    <appender name="WEBGENOME_CONSOLE" class="org.apache.log4j.ConsoleAppender">\par
        <param name="Target" value="System.out"/>\par
        <layout class="org.apache.log4j.PatternLayout">\par
            <param name="ConversionPattern" value="%d\{ABSOLUTE\} %-5p [%c\{1\}] %m%n"/>\par
        </layout>\par
    </appender>\par
\par
    <appender name="REMBRANDT_FILE" class="org.apache.log4j.DailyRollingFileAppender">\par
        <param name="File" value="$\{jboss.server.home.dir\}/log/rembrandtError.log"/>\par
        <param name="Append" value="true"/>\par
        <layout class="org.apache.log4j.PatternLayout">\par
            <param name="ConversionPattern" value="%d %-5p [%c] %m%n"/>\par
        </layout>\par
     </appender>\par
\par
\par
\pard\intbl\nowidctlpar\hyphpar0\sb60\sa60\sl240\slmult0\b\f1    <!-- ================ -->\par
   <!-- Limit categories -->\par
   <!-- ================ -->\par
    <!-- Default logger for Rembrandt. Used where no logger is declared -->\par
       <logger name="gov.nih.nci.rembrandt" additivity="false">\par
               <level value="ERROR"/>\par
               <appender-ref ref="REMBRANDT_FILE"/>\par
      </logger>\par
\par
       <!-- Default logger for caintegrator. Used where no logger is declared -->\par
       <logger name="gov.nih.nci.caintegrator" additivity="false">\par
               <level value="ERROR"/>\par
               <appender-ref ref="REMBRANDT_FILE"/>\par
      </logger>\par
\par
      <!-- Specific loggers for caIntegrator & Rembrandt.-->\par
      <logger name="gov.nih.nci.rembrandt.queryservice" additivity="false">\par
           <level value="DEBUG"/>\par
           <appender-ref ref="WEBGENOME_FILE"/>\par
           <appender-ref ref="WEBGENOME_CONSOLE"/>\par
      </logger>\par
      <logger name="gov.nih.nci.rembrandt.util.ThreadPool" additivity="false">\par
              <level value="DEBUG"/>\par
              <appender-ref ref="WEBGENOME_FILE"/>\par
              <appender-ref ref="WEBGENOME_CONSOLE"/>\par
      </logger>\par
\par
      <!-- Default logger for WebGenome. Used where no logger is declared -->\par
      <logger name="gov.nih.nci.caIntegrator.services" additivity="false">\par
        <level value="DEBUG"/>\par
        <appender-ref ref="WEBGENOME_FILE"/>\par
        <appender-ref ref="WEBGENOME_CONSOLE"/>\par
      </logger>\par
\par
      <!-- OJB LOGGING -->\par
      <!-- Default logger for OJB. Used where no logger is declared -->\par
      <logger name="org.apache.ojb" additivity="false">\par
        <level value="ERROR"/>\par
        <appender-ref ref="WEBGENOME_FILE"/>\par
        <appender-ref ref="WEBGENOME_CONSOLE"/>\par
      </logger>\par
     <!-- Logger for PersistenceBrokerFactory class -->\par
      <logger name="org.apache.ojb.broker.core.PersistenceBrokerImpl" additivity="false">\par
        <level value="ERROR"/>\par
        <appender-ref ref="WEBGENOME_FILE"/>\par
        <appender-ref ref="WEBGENOME_CONSOLE"/>\par
      </logger>\par
\par
      <!-- Logger for RepositoryXmlHandler, useful for debugging parsing of repository.xml -->\par
      <logger name="org.apache.ojb.broker.metadata.RepositoryXmlHandler" additivity="false">\par
        <level value="WARN"/>\par
        <appender-ref ref="WEBGENOME_FILE"/>\par
        <appender-ref ref="WEBGENOME_CONSOLE"/>\par
      </logger>\par
\par
      <!-- Logger for JdbcAccess, useful for debugging JDBC related problems -->\par
      <logger name="org.apache.ojb.broker.accesslayer.JdbcAccessImpl" additivity="false">\par
