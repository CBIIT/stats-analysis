    Created on : June 26, 2006
    Author     : Dana Zhang
    Description: Hibernate mapping file for SNPAssociationFinding
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"C:/hibernate-3.1/src/org/hibernate/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.analysis.snp">
   <class name="SNPAssociationFinding" 
    	table="SNP_ANALYSIS_FINDING_FACT"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.Long"
           column="ASSO_ANA_FACT_ID">
           <generator class="assigned" />
       </id>
       
       
        <!-- pvalue -->        
       <property name="pvalue" 
                 type="java.lang.Float" 
                 column="ASSO_ANA_PVALUE" 
                 insert="false" 
                 update="false" />
                 
       <!-- ASSO_ANA_RANK -->                 
       <property name="rank" 
                 type="java.lang.Integer" 
                 column="ASSO_ANA_RANK" 
                 insert="false" 
                 update="false" />                 
                   
    
            
     <!--  m-1 relationship -->
    
   <many-to-one name="snpAnnotation" lazy="false"  class="gov.nih.nci.caintegrator.domain.annotation.snp.SNPAnnotation" column="SNPANNO_ID" />
   <many-to-one name="snpAssociationAnalysis" lazy="false" class="gov.nih.nci.caintegrator.domain.analysis.snp.SNPAssociationAnalysis" column="ASSO_ANALYSIS_ID" />
      
      
	</class>
</hibernate-mapping>