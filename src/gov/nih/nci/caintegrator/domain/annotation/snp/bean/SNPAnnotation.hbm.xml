<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.annotation.snp.bean">
   <class name="SNPAnnotation" 
    	table="VARIATION_REPORTER"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           column="VARIATION_REPORTER_ID">
           <generator class="assigned" />
       </id>
       
       
        <!-- chromosomeLocation -->        
       <property name="chromosomeLocation" 
                 type="java.lang.Long"
                 column="SNP_PHYSICAL_POSITION" 
                 insert="false" 
                 update="false" />
                 
       <!-- chromosomeName -->                 
       <property name="chromosomeName" 
                 type="java.lang.String" 
                 column="SNP_CHROMOSOME" 
                 insert="false" 
                 update="false" />     
                 
        <!-- dbsnpBuild -->        
       <property name="dbsnpBuild" 
                 type="java.lang.String" 
                 column="SNP_DBSNP_BUILD" 
                 insert="false" 
                 update="false" />
                 
       <!-- dbsnpId -->                 
       <property name="dbsnpId" 
                 type="java.lang.String" 
                 column="DB_SNP_ID" 
                 insert="false" 
                 update="false" />    

       <property name="geneLocation"
                 type="java.lang.String"
                 column="SNP_CYTOBAND"
                 insert="false"
                 update="false" />
                 
        <!-- genomeBuild -->
       <property name="genomeBuild" 
                 column="SNP_GENOME_BUILD" 
                 insert="false" 
                 update="false" />
                 
       <set name="geneBiomarkerCollection" table="GENE_SNP_ASSO" batch-size="60">
               <cache usage="read-write" />
               <key column="SNPANNO_ID" />
               <many-to-many class="gov.nih.nci.caintegrator.domain.annotation.gene.bean.GeneBiomarker"
                        outer-join="true"  column="GENE_ID" />
       </set>



 

    <!--    TODO: This is same as "id"/SNPANNO_ID attribute (internally assigned):  So commented out.  RAM
       <property name="secondaryIdentifier" 
                 type="java.lang.String" 
                 column="SECONDARY_ID" 
                 insert="false" 
                 update="false" />                 
     -->                                 
                   
     <!-- here is for 1-m relationships -->  
    
     <set name="snpAssociationFindingCollection" lazy="true" inverse="true" batch-size="10">
      <cache usage="read-write" />
      <key column="SNPANNO_ID" />
      <one-to-many class="gov.nih.nci.caintegrator.domain.analysis.snp.bean.SNPAssociationFinding" />
     </set>   
     
<!--
      <set name="snpAssayCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="SNPANNO_ID" />
      <one-to-many class="gov.nih.nci.caintegrator.domain.annotation.snp.bean.SNPAssay" />
     </set>  
-->
       <set name="snpPanelCollection" table="SNP_ASSAY" lazy="true">
         <cache usage="read-write" />
         <key column="SNPANNO_ID" />
          <many-to-many class="gov.nih.nci.caintegrator.domain.annotation.snp.bean.SNPPanel" column="SNP_PANEL_ID" />
       </set>


      <set name="genotypeFindingCollection" lazy="true" inverse="true">
         <key column="SNPANNO_ID" />
         <one-to-many class="gov.nih.nci.caintegrator.domain.finding.variation.germline.bean.GenotypeFinding" />
     </set>   
     
      <set name="snpFrequencyCollection" lazy="true" inverse="true">
          <cache usage="read-write" />
          <key column="SNPANNO_ID" />
          <one-to-many class="gov.nih.nci.caintegrator.domain.finding.variation.snpFrequency.bean.SNPFrequencyFinding" />
     </set>  
	</class>
</hibernate-mapping>