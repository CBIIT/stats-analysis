<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="gov.nih.nci.caintegrator.domain.epidemiology.bean.DietaryConsumption" 
    	table="DIETARY_CONSUMPTION"
        lazy="true">

	    <id name="id" 
	    	column="DIETARY_CONSUMPTION_ID">
	        <generator class="native"/>
	    </id>
	    
        <property name="frequency" 
        	column="FREQUENCY"/>
        	
        <property name="name" 
        	column="NAME"/>
        	


	</class>
</hibernate-mapping>