    Description: Hibernate mapping file for LossOfExpressionIHCFinding
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

	
<hibernate-mapping package="gov.nih.nci.caintegrator.domain.finding.protein.ihc.bean">

 <union-subclass name="LossOfExpressionIHCFinding" 
        extends="gov.nih.nci.caintegrator.domain.finding.protein.ihc.bean.IHCFinding" 
    	table="IHC_LOSS_FACT"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.Long" 
           column="IHC_FID">
           <generator class="assigned" />
       </id>


       <!-- benignPresentValue -->  
        <property name="benignPresentValue"
                 type="java.lang.String"
                 column="BENIGN_PRESENT_VALUE"
                 insert="false"
                 update="false" />

        <!-- benignSum -->        
       <property name="benignSum" 
                 type="java.lang.Integer" 
                 column="BENIGN_SUM" 
                 insert="false" 
                 update="false" />
                 
       <!-- comments -->
       <property name="comments" 
                 type="java.lang.String" 
                 column="COMMENTS" 
                 insert="false" 
                 update="false" />
                 
        <!-- invasiveBenignDiff -->
       <property name="invasiveBenignDiff" 
                 type="java.lang.Integer" 
                 column="INVASIVE_BENIGN_DIFF" 
                 insert="false" 
                 update="false" />
                 
       <!-- invasiveSum -->        
       <property name="invasiveSum" 
                 type="java.lang.Integer" 
                 column="INVASIVE_SUM" 
                 insert="false" 
                 update="false" />
                                  
        <!-- lossResult -->        
       <property name="lossResult" 
                 type="java.lang.String" 
                 column="LOSS_RSLT" 
                 insert="false" 
                 update="false" />       
      
	</union-subclass>
</hibernate-mapping>