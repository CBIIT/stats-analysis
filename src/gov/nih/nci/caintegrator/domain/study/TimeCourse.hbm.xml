<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"C:/hibernate-3.1/src/org/hibernate/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.rembrandt.domain.cgems.study">
   <class name="TimeCourse" 
    	table="STUDY_TIMECOURSE_DIM"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.String" 
           column="TIMECOURSE_ID">
           <generator class="assigned" />
       </id>       
       
        <!-- name -->        
       <property name="name" 
                 type="java.lang.String" 
                 column="TIMECOURSE_NAME" 
         />
      
	</class>
</hibernate-mapping>