<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping
	package="gov.nih.nci.caintegrator.util">
	<class name="PlatformMapping" table="PLATFORM" lazy="true">

		<!-- id -->
		<id name="id" column="PLATFORM_ID">
			<generator class="assigned" />
		</id>
    
    
	    <property name="platformName" column="PLATFORM_NAME" />

		
		<property name="displayString" column="DISPLAY_STRING" />


		<property name="center" column="CENTER" />
		
	
		<property name="dataType" column="DATA_TYPE" />
		
		<property name="fileName" column="FILE_NAME" />
		
		<property name="annotationFileName" column="ANNOTATION_FILE_NAME" />
			
	</class>
</hibernate-mapping>